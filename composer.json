{
    "name": "guiu/filament-filter-presets",
    "description": "A Filament plugin to save and load filter presets",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "filters",
        "presets",
        "tables",
        "ui"
    ],
    "authors": [
        {
            "name": "Guiu",
            "email": "gerardguiu@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/support": "^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.16",
        "laravel/framework": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Guiu\\FilamentFilterPresets\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guiu\\FilamentFilterPresets\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Guiu\\FilamentFilterPresets\\FilamentFilterPresetsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
